{
    "name": "he426100/php-windows",
    "type": "library",
    "autoload": {
        "psr-4": {
            "He426100\\phpautogui\\": "src/phpautogui/src",
            "He426100\\phpmsgbox\\": "src/phpmsgbox/src"
        }
    },
    "replace": {
        "he426100/phpautogui": "*",
        "he426100/phpmsgbox": "*"
    },
    "authors": [
        {
            "name": "he426100"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^2.28",
        "phpstan/phpstan": "^1.9",
        "psy/psysh": "^0.12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "analyse": "@php vendor/bin/phpstan analyse --memory-limit=-1 -l 0 src"
    }
}
